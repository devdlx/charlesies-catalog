{
  "firebase": "charlesiesbeta",
  "public": "dist",
  "ignore": [
    "firebase.json",
    "**/.*",
    "**/node_modules/**"
  ],
  "rewrites": [{
      "source": "**/elements.vulcanized.html",
      "destination": "/elements/elements.vulcanized.html"
    },

    {
      "source": "**/lib.min.js",
      "destination": "/lib.min.js"
    }, {
      "source": "**/fonts/niconne-regular-charlesies.ttf",
      "destination": "/fonts/niconne-regular-charlesies.ttf"
    }, {
      "source": "**",
      "destination": "/index.html"
    }
  ]
}
